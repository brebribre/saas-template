{
  "name": "stratigo-job",
  "version": "1.0.0",
  "description": "Stratigo AI Agent Platform",
  "main": "index.js",
  "scripts": {
    "frontend": "cd frontend && npm run dev",
    "backend": "cd backend && source venv/bin/activate && flask run",
    "dev": "concurrently \"npm run frontend\" \"npm run backend\"",
    "install-all": "npm install && cd frontend && npm install && cd ../backend && python3 -m venv venv && source venv/bin/activate && pip install -r requirements.txt",
    "build": "cd frontend && npm run build",
    "start": "cd backend && source venv/bin/activate && flask run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brebribre/stratigo-job.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brebribre/stratigo-job/issues"
  },
  "homepage": "https://github.com/brebribre/stratigo-job#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "tailwindcss": "^3.4.17"
  }
}
